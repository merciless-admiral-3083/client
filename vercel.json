{
    "ignoreBuildStep": [
      "server/**"
    ],
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist/public" }
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://your-backend-url.com/api/$1"
      }
    ]
  }
  